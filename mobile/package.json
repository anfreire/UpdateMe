{
  "name": "update-me",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-start": "rm -rf android/build android/app/build android/app/src/release && cd android && ./gradlew clean && cd .. && react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "rm_app_adb": "adb uninstall com.updateme > /dev/null",
    "gnome_clean_start": "gnome-terminal -- bash -c 'cd /home/anfreire/Documents/Projects/Android/updateMe/mobile && npm run clean-start'",
    "gnome_android": "gnome-terminal --wait -- bash -c 'cd /home/anfreire/Documents/Projects/Android/updateMe/mobile && npm run android'",
    "_scrcpy": "nohup scrcpy --no-audio  &",
    "scrcpy": "npm run _scrcpy; rm -rf nohup.out",
    "restart": "npm run rm_app_adb && npm run gnome_clean_start && sleep 20 && npm run gnome_android && npm run scrcpy && rm -rf nohup.out",
    "rm-rn": "rm node_modules/rn-fetch-blob/react-native.config.js",
    "reinstall-modules": "rm -rf node_modules && npm install",
    "reset-watch": "watchman watch-del '/home/anfreire/Documents/Projects/Android/updateMe/mobile' ; watchman watch-project '/home/anfreire/Documents/Projects/Android/updateMe/mobile'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-firebase/app": "^18.7.2",
    "@react-native-firebase/firestore": "^18.7.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-launcher-kit": "^1.0.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-vector-icons": "^10.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "rn-fetch-blob": "^0.12.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
